{
  "includes": [ "chips/gap/gap.json" ],

  "board": {
    "name": "ai_deck",
    "devices": {
      "hyper": {
        "include" : "devices/hyper.json",
        "cs": 0
      },
      "camera": {
        "include": "devices/camera.json",
        "interface": "cpi0",
        "ctrl_interface": "i2c1"
      }
    }
  },

  "debug_bridge": {
    "config": {
      "cable": {
        "system_reset_gpio": 4,
        "jtag_reset_gpio": 6,
        "reverse_reset": true
      }
    }
  }

}
