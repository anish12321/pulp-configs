{
  "runner": {
    "boot_from_flash": true,
    "boot-mode": "rom"
  },
  
  "loader": {
    "boot": {
      "mode": "rom"
    }
  },

  "rt": {
    "iodevs"          : {
      "uart": {
        "baudrate": "115200"
      }
    }
  },

  "system_tree": {
    "debug_bridge": {
      "boot-mode": "jtag"
    },
   "board": {
     "chip": {
       "soc": {
         "quiddikey": {
           "includes": [ "../../ips/vendors/iid/quiddikey_v1.json"]
         },
         "csi2": {
           "includes": [ "../../ips/vendors/lnt/csi2_v1.json"]
         }
       }
     }
   }
  }
}

