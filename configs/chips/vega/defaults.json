{
  "runner": {
    "boot_from_flash": true,
    "boot-mode": "rom"
  },
  
  "loader": {
    "boot": {
      "mode": "rom"
    }
  },

  "system_tree": {
    "debug_bridge": {
      "boot-mode": "jtag"
    },
    "board": {
      "chip": {
        "soc": {
          "rtc": {
            "includes": [ "ips/rtc/rtc.json" ]
          },
          "quiddikey": {
            "includes": [ "ips/vendors/iid/quiddikey_v1.json"]
          },
          "csi2": {
            "includes": [ "ips/vendors/lnt/csi2_v1.json"]
          }
        }
      }
    }
  }
}

