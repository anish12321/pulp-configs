{
  "name": "bigpulp-zu9eg",
  "pulp_chip_family": "bigpulp",
  "pulp_chip_version": 0,
  "boot_from_rom": false,
  "vp_class": "pulp/chip",
  "hal_files": [
    "hal/chips/bigpulp/pulp.h"
  ],
  "archi_files": [
    "archi/chips/bigpulp/pulp.h",
    "archi/chips/bigpulp/memory_map.h",
    "archi/chips/bigpulp/properties.h",
    "archi/chips/bigpulp/apb_soc.h"
  ],
  "vp_comps": [
    "padframe",
    "soc_clock",
    "soc"
  ],
  "vp_ports": [
    "ref_clock",
    "jtag0",
    "spim0",
    "uart0"
  ],
  "vp_bindings": [
    [
      "self->ref_clock",
      "padframe->ref_clock_pad"
    ],
    [
      "self->jtag0",
      "padframe->jtag0_pad"
    ],
    [
      "padframe->ref_clock",
      "soc->ref_clock"
    ],
    [
      "padframe->spim0_pad",
      "self->spim0"
    ],
    [
      "padframe->uart0_pad",
      "self->uart0"
    ],
    [
      "padframe->jtag0",
      "soc->jtag0"
    ],
    [
      "soc_clock->out",
      "padframe->clock"
    ],
    [
      "soc_clock->out",
      "soc->clock"
    ],
    [
      "soc->spim0",
      "padframe->spim0"
    ],
    [
      "soc->uart0",
      "padframe->uart0"
    ],
    [
      "soc->fll_soc_clock",
      "soc_clock->clock_in"
    ]
  ],
  "padframe": {
    "includes": [
      "templates/chips/bigpulp-zu9eg/padframe.json"
    ],
    "vp_ports": [
      "ref_clock",
      "spim0_pad",
      "uart0_pad",
      "jtag0",
      "ref_clock_pad",
      "clock",
      "spim0",
      "uart0",
      "jtag0_pad"
    ]
  },
  "soc_clock": {
    "vp_class": "vp/clock_domain",
    "frequency": 50000000,
    "vp_ports": [
      "out",
      "clock_in"
    ]
  },
  "soc": {
    "includes": [
      "soc.json"
    ],
    "vp_ports": [
      "spim0",
      "uart0",
      "fll_soc_clock",
      "ref_clock",
      "jtag0",
      "clock"
    ]
  }
}
