{
  "vp_comps": [
    "system_tree"
  ],
  "system_tree": {
    "board": {
      "vp_comps": [
        "spiflash"
      ],
      "tb_comps": [
        "jtag_proxy",
        "camera"
      ],
      "vp_bindings": [
        [
          "dpi->jtag0",
          "chip->jtag0_pad"
        ]
      ],
      "tb_bindings": [
        [
          "chip->jtag0",
          "jtag_proxy->jtag"
        ],
        [
          "chip->ctrl",
          "jtag_proxy->ctrl"
        ]
      ],
      "chip": {
        "vp_ports": [
          "jtag0",
          "ctrl",
          "jtag0_pad"
        ],
        "vp_bindings": [
          [
            "self->jtag0_pad",
            "padframe->jtag0_pad"
          ]
        ],
        "padframe": {
          "vp_ports": [
            "jtag0_pad"
          ]
        }
      },
      "dpi": {
        "vp_ports": [
          "jtag0"
        ]
      },
      "jtag_proxy": {
        "type": "dpi",
        "active": false,
        "module": "jtag_proxy.so",
        "verbose": false,
        "port": 37539,
        "vp_ports": [
          "jtag",
          "ctrl"
        ]
      },
      "camera": {
        "type": "dpi",
        "model": "himax",
        "module": "camera.so",
        "color-mode": "gray"
      },
      "spiflash": {
        "vp_class": "devices/spiflash/spiflash",
        "type": "spiflash",
        "size": "0x00800000",
        "fs": {
          "files": [],
          "encrypt": false,
          "aes_key": 0,
          "aes_iv": 0
        }
      }
    }
  }
}
