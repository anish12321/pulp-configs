{
  "vp_comps": [
    "system_tree"
  ],
  "system_tree": {
    "board": {
      "vp_comps": [
        "spiflash",
        "spiflash_clock"
      ],
      "tb_comps": [
        "jtag_proxy",
        "uart",
        "camera"
      ],
      "vp_bindings": [
        [
          "chip->uart0",
          "dpi->uart0"
        ],
        [
          "chip->spim0_cs0",
          "spiflash->cs"
        ],
        [
          "chip->spim0_cs0_data",
          "spiflash->input"
        ],
        [
          "dpi->jtag0",
          "chip->jtag0_pad"
        ],
        [
          "dpi->cpi0",
          "chip->cpi0_pad"
        ],
        [
          "spiflash_clock->out",
          "spiflash->clock"
        ]
      ],
      "tb_bindings": [
        [
          "chip->uart0",
          "uart->uart"
        ],
        [
          "chip->jtag0",
          "jtag_proxy->jtag"
        ],
        [
          "chip->ctrl",
          "jtag_proxy->ctrl"
        ],
        [
          "camera->cpi",
          "chip->cpi0"
        ]
      ],
      "chip": {
        "vp_ports": [
          "uart0",
          "jtag0",
          "ctrl",
          "jtag0_pad",
          "cpi0_pad",
          "spim0_cs0",
          "spim0_cs0_data",
          "cpi0"
        ],
        "vp_bindings": [
          [
            "self->jtag0_pad",
            "padframe->jtag0_pad"
          ],
          [
            "self->cpi0_pad",
            "padframe->cpi0_pad"
          ],
          [
            "padframe->uart0_pad",
            "self->uart0"
          ]
        ],
        "padframe": {
          "vp_ports": [
            "uart0_pad",
            "jtag0_pad",
            "cpi0_pad"
          ]
        }
      },
      "dpi": {
        "vp_ports": [
          "jtag0",
          "cpi0",
          "uart0"
        ]
      },
      "jtag_proxy": {
        "type": "dpi",
        "active": false,
        "module": "jtag_proxy.so",
        "verbose": false,
        "port": 37539,
        "vp_ports": [
          "jtag",
          "ctrl"
        ]
      },
      "uart": {
        "type": "dpi",
        "module": "uart.so",
        "verbose": false,
        "baudrate": 625000,
        "loopback": true,
        "stdout": false,
        "tx_file": "tx_uart.log",
        "vp_ports": [
          "uart"
        ]
      },
      "camera": {
        "type": "dpi",
        "model": "himax",
        "module": "camera.so",
        "vp_ports": [
          "cpi"
        ]
      },
      "spiflash": {
        "vp_class": "devices/spiflash/spiflash",
        "type": "spiflash",
        "size": "0x00800000",
        "fs": {
          "files": [],
          "encrypt": false,
          "aes_key": 0,
          "aes_iv": 0
        },
        "vp_ports": [
          "cs",
          "input",
          "clock"
        ]
      },
      "spiflash_clock": {
        "vp_class": "vp/clock_domain",
        "frequency": 50000000,
        "vp_ports": [
          "out"
        ]
      }
    }
  }
}
